{
  "name": "northrook/symfony-core",
  "type": "library",
  "description": "Various support classes for Symfony.",
  "license": "MIT",
  "authors": [
    {
      "name": "Martin",
      "email": "mn@northrook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Northrook\\Symfony\\": "src/"
    }
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=8.2",
    "northrook/core": "dev-main",
    "symfony/console": "*",
    "symfony/filesystem": "*"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.12"
  }
}